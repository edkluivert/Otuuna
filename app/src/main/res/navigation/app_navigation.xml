<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.kluivert.otuuna.Appfragments.Dashboard"
        android:label="Home"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_home_to_events"
            app:destination="@id/events" />
    </fragment>
    <fragment
        android:id="@+id/about"
        android:name="com.kluivert.otuuna.Appfragments.About"
        android:label="About"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/faq"
        android:name="com.kluivert.otuuna.Appfragments.FAQ"
        android:label="FAQ"
        tools:layout="@layout/fragment_f_a_q" />
    <fragment
        android:id="@+id/events"
        android:name="com.kluivert.otuuna.Appfragments.Events"
        android:label="fragment_events"
        tools:layout="@layout/fragment_events" />
</navigation>