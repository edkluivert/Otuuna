<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_navigation"
    app:startDestination="@id/home">

    <fragment
        android:id="@+id/home"
        android:name="com.kluivert.otuuna.ui.mainFrags.Dashboard"
        android:label="Home"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_home_to_profileEdit"
            app:destination="@id/profileEdit" />

    </fragment>
    <fragment
        android:id="@+id/profile"
        android:name="com.kluivert.otuuna.ui.mainFrags.Profile"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/history"
        android:name="com.kluivert.otuuna.ui.mainFrags.History"
        android:label="History"
        tools:layout="@layout/fragment_history" />
    <fragment
        android:id="@+id/cart"
        android:name="com.kluivert.otuuna.ui.mainFrags.Cart"
        android:label="Cart"
        tools:layout="@layout/fragment_cart" />
    <fragment
        android:id="@+id/profileEdit"
        android:name="com.kluivert.otuuna.Appfragments.ProfileEdit"
        android:label="fragment_profile_edit"
        tools:layout="@layout/fragment_profile_edit">

    </fragment>
</navigation>